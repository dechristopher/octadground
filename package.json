{
  "name": "octadground",
  "version": "1.0.3",
  "description": "lioctad.org octad ui",
  "type": "commonjs",
  "main": "octadground.js",
  "typings": "octadground.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "17",
    "@rollup/plugin-typescript": "8",
    "@typescript-eslint/eslint-plugin": "4",
    "@typescript-eslint/parser": "4",
    "eslint": "7",
    "prettier": "2",
    "rollup": "2",
    "rollup-plugin-terser": "7",
    "tslib": "2.4.0",
    "typescript": "4"
  },
  "scripts": {
    "prepare": "npm run compile",
    "compile": "tsc --outDir . --sourceMap --declaration",
    "lint": "eslint src/*.ts",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "dist": "rollup --config"
  },
  "files": [
    "/*.js",
    "/*.d.ts",
    "/*.js.map",
    "/assets/*.css",
    "/src/*.ts",
    "!/rollup.config.js"
  ],
  "repository": "https://github.com/dechristopher/octadground",
  "keywords": [
    "octad",
    "lioctad",
    "lioctad.org",
    "board",
    "ui",
    "typescript"
  ],
  "author": "Andrew DeChristopher (https://github.com/dechristopher)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/dechristopher/octadground/issues"
  }
}
